{
  "expo": {
    "name": "Espresso",
    "slug": "espresso",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "scheme": "privy-expo",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/Raleway-VariableFont.ttf"]
        }
      ]
    ],
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.privy.expo",
      "supportsTablet": true
    },
    "android": {
      "package": "com.privy.expo",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "privyAppId": "<your-app-id>"
    }
  }
}
