{
  "expo": {
    "name": "Espresso",
    "slug": "espresso",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "scheme": "privy-expo",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "garden.builders.espresso",
      "supportsTablet": true
    },
    "android": {
      "package": "garden.builders.espresso",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "privyAppId": "clrgh1bz400cijs0gbyk46urk"
    }
  }
}
